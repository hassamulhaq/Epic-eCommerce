{
    "name": "hassamulhaq/epic-ecommerce",
    "type": "project",
    "description": "A free and open-source ecommerce application.",
    "keywords": [
        "ecommerce",
        "online store",
        "estore"
    ],
    "homepage": "https://epic-ecommerce.hassam.dev",
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "akaunting/laravel-money": "^3.1.2",
        "algolia/algoliasearch-client-php": "^3.3",
        "cviebrock/eloquent-sluggable": "^9.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "laravel/framework": "^v9.27.0",
        "laravel/sanctum": "^v3.0.1",
        "laravel/tinker": "^2.7.2",
        "livewire/livewire": "^2.10",
        "opcodesio/log-viewer": "^1.2.8",
        "ramsey/uuid": "^4.5",
        "spatie/laravel-flash": "^1.9",
        "spatie/laravel-medialibrary": "10.4.5",
      "ext-http": "*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "fakerphp/faker": "^1.20.0",
        "laravel/breeze": "^1.12.0",
        "laravel/pint": "^1.1.2",
        "laravel/sail": "^1.15.4",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.3.0",
        "phpunit/phpunit": "^9.5.24",
        "spatie/laravel-ignition": "^1.4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
